<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ActivityPhone">
    <GridLayout
        android:layout_width="220dp"
        android:layout_height="220dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.554"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.031"
        tools:ignore="MissingConstraints">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal"
            tools:ignore="MissingConstraints">
            <EditText
                android:id="@+id/editText"
                android:layout_width="145dp"
                android:layout_height="30dp"
                android:layout_marginStart="5dp"
                android:background="@color/teal_200"
                android:hint="@string/editText"
                android:importantForAutofill="no"
                android:inputType="textAutoComplete|textAutoCorrect|textUri|textMultiLine"
                android:text=""
                android:textColorHint="@color/grey"
                android:textSize="15sp" />
            <Button
                android:id="@+id/btnGO"
                android:layout_width="55dp"
                android:layout_height="40dp"
                android:layout_marginStart="10dp"
                android:text="@string/btnGO"
                android:textColor="@color/black"
                android:textSize="15sp" />
        </LinearLayout>
        <ImageButton
            android:id="@+id/ib1"
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/ib1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.29"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.24"
            app:srcCompat="@drawable/google" />
        <ImageButton
            android:id="@+id/ib2"
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:layout_row="1"
            android:layout_column="0"
            android:layout_gravity="end"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/ib2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.743"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.24"
            app:srcCompat="@drawable/yandex" />
        <ImageButton
            android:id="@+id/ib3"
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/ib3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.29"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.64"
            app:srcCompat="@drawable/youtube" />
        <ImageButton
            android:id="@+id/ib4"
            android:layout_width="100dp"
            android:layout_height="80dp"
            android:layout_row="2"
            android:layout_column="0"
            android:layout_gravity="end"
            android:layout_marginTop="10dp"
            android:contentDescription="@string/ib4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.743"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.64"
            app:srcCompat="@drawable/duckduckgo" />
    </GridLayout>
</androidx.constraintlayout.widget.ConstraintLayout>